<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners
                android:radius="32dp" />
            <solid android:color="#FFFFFF" />
            <stroke
                android:width="1dp"
                android:color="#CCCCCC" />
            <padding
                android:left="14dp"
                android:right="24dp"
                android:top="8dp"
                android:bottom="8dp"/>
        </shape>
    </item>
</layer-list>
